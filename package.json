{
  "name": "memento",
  "version": "1.0.0",
  "description": "Team memento's IT project 2019",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "lint": "tslint --project tsconfig.json",
    "gql": "cd old-graphql && graphql-codegen --config codegen.yml",
    "update": "npm i && cd client && npm i",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "build-client": "npm run build --prefix client",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "client": "npm start --prefix client",
    "postinstall": "rimraf dist && tsc -p tsconfig.build.json",
    "server": "nodemon --exec ts-node server.ts",
    "test": "jest",
    "start:prod": "NODE_ENV=production nodemon --exec ts-node server.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:dev": "nodemon"
  },
  "engines": {
    "node": "8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trontor/memento.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Trontor/memento/issues"
  },
  "homepage": "https://github.com/Trontor/memento#readme",
  "dependencies": {
    "@hapi/joi": "^15.1.1",
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/graphql": "^6.4.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/serve-static": "^1.0.1",
    "apollo-link-error": "^1.1.11",
    "apollo-server-express": "^2.9.2",
    "aws-sdk": "^2.528.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "firebase": "^6.3.5",
    "firebase-admin": "^8.3.0",
    "graphql": "^14.4.2",
    "graphql-tools": "^4.0.5",
    "graphql-upload": "^8.0.7",
    "mongoose": "^5.6.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.3.3",
    "type-graphql": "^0.17.5",
    "react-country-region-selector": "^1.4.4",
    "react-day-picker": "^7.3.2",
    "react-datepicker": "^2.9.6",
    "react-geosuggest": "^2.12.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.6.1",
    "@graphql-codegen/introspection": "^1.5.0",
    "@graphql-codegen/typescript": "1.5.0",
    "@graphql-codegen/typescript-resolvers": "^1.5.0",
    "@nestjs/testing": "^6.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/graphql-upload": "^8.0.3",
    "@types/hapi__joi": "^15.0.3",
    "@types/jest": "^24.0.18",
    "@types/mongoose": "^5.5.16",
    "@types/node": "^12.7.2",
    "@types/nodemailer": "^6.2.1",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/uuid": "^3.4.5",
    "apollo-server-testing": "^2.8.2",
    "concurrently": "^4.1.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
}

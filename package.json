{
  "name": "memento",
  "version": "1.0.0",
  "description": "Team memento's IT project 2019",
  "main": "lib/server.js",
  "scripts": {
    "start": "node lib/server.js",
    "lint": "tslint --project tsconfig.json",
    "gql": "cd graphql && graphql-codegen --config codegen.yml",
    "update": "npm i && npm i --prefix client",
    "build": "tsc",
    "build-client": "npm run build --prefix client",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "client": "npm start --prefix client",
    "server": "nodemon --exec ts-node server.ts",
    "test": "jest"
  },
  "engines": {
    "node": "8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trontor/memento.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Trontor/memento/issues"
  },
  "homepage": "https://github.com/Trontor/memento#readme",
  "dependencies": {
    "apollo-server-express": "^2.8.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "firebase": "^6.3.5",
    "firebase-admin": "^8.3.0",
    "firebase-functions": "^3.2.0",
    "firebase-functions-test": "^0.1.6",
    "graphql": "^14.4.2",
    "jest": "^24.8.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.6.1",
    "@graphql-codegen/introspection": "^1.5.0",
    "@graphql-codegen/typescript": "1.5.0",
    "@graphql-codegen/typescript-resolvers": "^1.5.0",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.18",
    "@types/uuid": "^3.4.5",
    "@types/node": "^12.7.2",
    "apollo-server-testing": "^2.8.2",
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
}
